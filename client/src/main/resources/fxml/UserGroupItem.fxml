<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<HBox alignment="CENTER_LEFT" prefWidth="205.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.imtp.client.controller.UserGroupItemController">
    <children>
        <ImageView fx:id="groupImg" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" style="-fx-effect: dropshadow(three-pass-box, rgba(149, 157, 165, 0.2), 10, 0, 0, 0);"/>
      <HBox alignment="CENTER_LEFT" prefWidth="205.0">
         <children>
              <Label fx:id="groupLabel" style="-fx-font-size: 14px"/>
         </children>
         <padding>
            <Insets left="10.0" />
         </padding>
      </HBox>
    </children>
</HBox>
